SHELL=/bin/bash

#######################################################################
# hysdsops user crontab
#######################################################################

@weekly hysdsops /export/home/hysdsops/cmr_audit/opera-sds-pcm/conf/sds/files/metrics/cron/run_cmr_audit.sh --hls | logger -i -p user.notice -t cmr_audit_hls-task
@weekly hysdsops /export/home/hysdsops/cmr_audit/opera-sds-pcm/conf/sds/files/metrics/cron/run_cmr_audit.sh --slc | logger -i -p user.notice -t cmr_audit_slc-task